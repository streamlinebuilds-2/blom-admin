{
  "name": "Bundle",
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "status": {
      "type": "string",
      "enum": [
        "active",
        "draft",
        "archived"
      ],
      "default": "draft"
    },
    "price": {
      "type": "number",
      "description": "Bundle price in cents"
    },
    "compare_at_price": {
      "type": "number"
    },
    "short_description": {
      "type": "string"
    },
    "long_description": {
      "type": "string"
    },
    "image_url": {
      "type": "string"
    }
  },
  "required": [
    "name",
    "price"
  ]
}